{
  "name": "pm2-gelf",
  "version": "0.0.3",
  "description": "PM2 Logging to GELF for GrayLog",
  "main": "app.js",
  "dependencies": {
    "gelf": "^2.0.1",
    "pm2": "^3.2.2",
    "pmx": "^1.6.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nativesrc/pm2-gelf.git"
  },
  "config": {
    "graylogPort": 12201,
    "graylogHostname": "127.0.0.1",
    "connection": "wan",
    "maxChunkSizeWan": 1420,
    "maxChunkSizeLan": 8154
  },
  "license": "MIT"
}
